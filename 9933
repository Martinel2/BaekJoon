#include <iostream>
#include <string>
#include <algorithm>
#include <map>
using namespace std;
 
map<string, bool> visited;
 
int main(void)
{
        ios_base::sync_with_stdio(0);
        cin.tie(0);
        int N;
        cin >> N;
 
        for (int i = 0; i < N; i++)
        {
                 string s;
                 cin >> s;
 
                 string temp = s;
                 reverse(temp.begin(), temp.end());
                        
                 //팰린드롬인 경우 혹은 기존에 뒤집은 문자열이 나온 경우
                 if (s == temp || visited.count(temp))
                 {
                         cout << s.length() << " " << s[s.length() / 2] << "\n";
                         break;
                 }
                 else
                         visited[s] = true;
        }
        return 0;
}
